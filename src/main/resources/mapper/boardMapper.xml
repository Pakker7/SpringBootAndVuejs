<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.psk.board.board.BoardDAO">
    <select id="selectList" resultType="com.test.psk.board.board.BoardVO">
        SELECT  NO
                , TITLE
                , CONTENTS
                , WRITER
                , DATE
        FROM BOARD
        ORDER BY DATE DESC
    </select>
</mapper>